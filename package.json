{
	"name": "sensethenlove",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"localWrite": "bash ./src/lib/scripts/localWrite.sh",
		"dev": "bash ./src/lib/scripts/dev.sh",
		"qaWrite": "bash ./src/lib/scripts/qaWrite.sh",
		"qaPush": "bash ./src/lib/scripts/qaPush.sh",
		"qaDeploy": "bash ./src/lib/scripts/qaDeploy.sh",
		"qaLogs": "wrangler tail --env qa",
		"prodWrite": "bash ./src/lib/scripts/prodWrite.sh",
		"prodPush": "bash ./src/lib/scripts/prodPush.sh",
		"prodDeploy": "bash ./src/lib/scripts/prodDeploy.sh",
		"prodLogs": "wrangler tail",
		"schema": "pnpm qa-write && pnpm prisma db push",
		"studio": "pnpm prisma generate && pnpm prisma studio",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20230228.0",
		"@poppanator/sveltekit-svg": "^2.1.2",
		"@prisma/client": "^4.11.0",
		"@sveltejs/adapter-cloudflare-workers": "^1.1.0",
		"@sveltejs/kit": "^1.10.0",
		"@types/node": "^18.14.6",
		"@types/sanitize-html": "^2.8.1",
		"buffer": "^6.0.3",
		"prisma": "^4.11.0",
		"sanitize-html": "^2.10.0",
		"sass": "^1.58.3",
		"svelte": "^3.55.1",
		"svelte-check": "^3.0.4",
		"ts-node": "^10.9.1",
		"typescript": "^4.9.5",
		"vite": "^4.1.4",
		"wrangler": "^2.12.0",
		"zod": "^3.20.6"
	}
}
