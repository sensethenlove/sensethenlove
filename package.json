{
	"name": "sensethenlove",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"dev": "pnpm prisma generate && vite dev",
		"push": "pnpm check && node ./prisma/validateLive.js && git push",
		"live": "pnpm check && node ./prisma/validateLive.js && pnpm prisma generate --data-proxy && pnpm build && pnpm wrangler publish",
		"schema": "node ./prisma/validatePushSchema.js && pnpm prisma db push",
		"studio": "pnpm prisma generate && pnpm prisma studio",
		"logs": "wrangler tail",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20230221.0",
		"@poppanator/sveltekit-svg": "^2.1.2",
		"@prisma/client": "^4.10.1",
		"@sveltejs/adapter-cloudflare-workers": "^1.0.6",
		"@sveltejs/kit": "^1.8.4",
		"@types/sanitize-html": "^2.8.0",
		"buffer": "^6.0.3",
		"prisma": "^4.10.1",
		"sanitize-html": "^2.10.0",
		"sass": "^1.58.3",
		"svelte": "^3.55.1",
		"svelte-check": "^3.0.4",
		"typescript": "^4.9.5",
		"vite": "^4.1.4",
		"wrangler": "^2.11.1",
		"zod": "^3.20.6"
	}
}
