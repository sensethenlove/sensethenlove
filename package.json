{
  "name": "sensethenlove",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "write": "bash ./src/lib/scripts/write.sh",
    "devWrite": "env=local host=http://localhost:5173 db=mainPlanetScale pnpm write",
    "dev": "bash ./src/lib/scripts/dev.sh",
    "qaWrite": "env=qa host=https://qa.sensethenlove.com db=qaPrisma pnpm write",
    "qaPush": "bash ./src/lib/scripts/qaPush.sh",
    "qaDeploy": "bash ./src/lib/scripts/qaDeploy.sh",
    "qaLogs": "pnpm wrangler tail --env qa",
    "qaData": "db=qaPlanetScale bash ./src/lib/scripts/data.sh",
    "mainWrite": "env=main host=https://sensethenlove.com db=mainPrisma pnpm write",
    "mainPush": "bash ./src/lib/scripts/mainPush.sh",
    "mainDeploy": "bash ./src/lib/scripts/mainDeploy.sh",
    "mainData": "db=mainPlanetScale bash ./src/lib/scripts/data.sh",
    "mainLogs": "pnpm wrangler tail",
    "schemaDeploy": "bash ./src/lib/scripts/schemaDeploy.sh",
    "preview": "pnpm vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230419.0",
    "@poppanator/sveltekit-svg": "^2.1.2",
    "@prisma/client": "^4.13.0",
    "@sensethenlove/env-write": "^1.1.1",
    "@sensethenlove/get-form-entries": "^1.0.1",
    "@sensethenlove/get-relative-time": "^1.0.0",
    "@sensethenlove/global-style": "^1.0.7",
    "@sensethenlove/jwt": "^1.0.6",
    "@sensethenlove/loop-backwards": "^1.0.9",
    "@sensethenlove/slug": "^1.0.2",
    "@sensethenlove/svelte-loading-anchor": "^1.0.2",
    "@sensethenlove/svelte-modal": "^1.0.2",
    "@sensethenlove/svelte-turnstile": "^1.1.5",
    "@sensethenlove/toast": "^1.0.8",
    "@sveltejs/adapter-cloudflare-workers": "^1.1.1",
    "@sveltejs/kit": "^1.15.10",
    "@types/node": "^18.16.3",
    "@types/sanitize-html": "^2.9.0",
    "buffer": "^6.0.3",
    "prisma": "^4.13.0",
    "sanitize-html": "^2.10.0",
    "sass": "^1.62.1",
    "svelte": "^3.58.0",
    "svelte-check": "^3.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "vite": "^4.3.4",
    "wrangler": "^2.19.0",
    "zod": "^3.21.4"
  }
}
