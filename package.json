{
	"name": "sensethenlove",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "npm run prisma-to-prod && vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"prisma-to-dev": "node ./prisma/setPrismaUrl.js DEV_DATABASE_URL && npx prisma generate",
		"prisma-to-prod": "node ./prisma/setPrismaUrl.js PROD_DATABASE_URL && npx prisma generate",
		"prisma-to-cloud": "node ./prisma/setPrismaUrl.js PRISMA_DATABASE_URL && npx prisma generate --data-proxy"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20230115.0",
		"@poppanator/sveltekit-svg": "^2.1.2",
		"@prisma/client": "^4.10.1",
		"@sveltejs/adapter-cloudflare-workers": "^1.0.6",
		"@sveltejs/kit": "^1.5.0",
		"buffer": "^6.0.3",
		"prisma": "^4.10.1",
		"sass": "^1.58.0",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.0",
		"wrangler": "^2.9.1",
		"zod": "^3.20.6"
	}
}
