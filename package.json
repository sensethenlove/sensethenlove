{
	"name": "sensethenlove",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"write": "bash ./src/lib/scripts/write.sh",
		"devWrite": "env=local db=mainPlanetScale pnpm write",
		"dev": "bash ./src/lib/scripts/dev.sh",
		"qaWrite": "env=qa db=qaPrisma pnpm write",
		"qaPush": "bash ./src/lib/scripts/qaPush.sh",
		"qaDeploy": "bash ./src/lib/scripts/qaDeploy.sh",
		"qaLogs": "pnpm wrangler tail --env qa",
		"qaData": "db=qaPlanetScale bash ./src/lib/scripts/data.sh",
		"mainWrite": "env=main db=mainPrisma pnpm write",
		"mainPush": "bash ./src/lib/scripts/mainPush.sh",
		"mainDeploy": "bash ./src/lib/scripts/mainDeploy.sh",
		"mainData": "db=mainPlanetScale bash ./src/lib/scripts/data.sh",
		"mainLogs": "pnpm wrangler tail",
		"schemaDeploy": "bash ./src/lib/scripts/schemaDeploy.sh",
		"preview": "pnpm vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20230307.0",
		"@poppanator/sveltekit-svg": "^2.1.2",
		"@prisma/client": "^4.11.0",
		"@sveltejs/adapter-cloudflare-workers": "^1.1.0",
		"@sveltejs/kit": "^1.10.0",
		"@types/node": "^18.14.6",
		"@types/sanitize-html": "^2.8.1",
		"buffer": "^6.0.3",
		"prisma": "^4.11.0",
		"sanitize-html": "^2.10.0",
		"sass": "^1.58.3",
		"svelte": "^3.55.1",
		"svelte-check": "^3.1.0",
		"ts-node": "^10.9.1",
		"typescript": "^4.9.5",
		"vite": "^4.1.4",
		"wrangler": "^2.12.1",
		"zod": "^3.21.4"
	}
}
